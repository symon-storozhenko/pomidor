#!/Users/myco/PycharmProjects/tomato3/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Pepper==0.0.3','console_scripts','PepperLex'
__requires__ = 'Pepper==0.0.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Pepper==0.0.3', 'console_scripts', 'PepperLex')()
    )
